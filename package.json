{
  "name": "modbus2mqtt",
  "version": "0.19.0",
  "sshserver": "homeassistant@homeassistant.lan",
  "homepage": "https://github.com/modbus2mqtt/server/README.md",
  "description": "server for modbus2mqtt for REST API of configuration and publishing modbus values to mqtt",
  "keywords": [
    "modbus2mqtt",
    "specification",
    "validation"
  ],
  "bugs": "https://github.com/modbus2mqtt/server/issues",
  "packageManager": "pnpm@10.28.2",
  "type": "module",
  "bin": {
    "modbus2mqtt": "bin/modbus2mqtt"
  },
  "author": "Volkmar Nissen <volkmar.nissen@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=22.11.0"
  },
  "files": [
    "bin/",
    "dist/server/",
    "dist/shared/",
    "dist/specification/",
    "dist/frontend/",
    "specifications/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modbus2mqtt/server.git"
  },
  "credits": "multiple choice by Studio 365 from <a href='https://thenounproject.com/browse/icons/term/multiple-choice/' target='_blank' title='multiple choice Icons'>Noun Project</a> (CC BY 3.0)",
  "scripts": {
    "ng": "npx ng",
    "test": "pushd backend && pnpm run test && popd && pushd frontend && pnpm run test && popd && pushd specification && pnpm run test && popd",
    "server": "pnpm run build && node dist/server/modbus2mqtt.js -s ../ssl -y ../yaml-dir",
    "lint": "eslint --cache --ext .ts,.tsx,.js,.jsx --ignore-pattern 'frontend/**' --format stylish src",
    "lint:fix": "eslint --cache --fix --ext .ts,.tsx,.js,.jsx --ignore-pattern 'frontend/**' --format stylish src",
    "e2e:start": "./e2e/servers/testall.py startServers",
    "e2e:restart": "./e2e/servers/testall.py restartServers",
    "e2e:stop": "./e2e/servers/testall.py killServers",
    "e2e:test": "./e2e/servers/testall.py test",
    "e2e:playwright": "npx playwright test",
    "e2e:playwright:headed": "npx playwright test --headed",
    "e2e:playwright:debug": "PWDEBUG=1 npx playwright test --headed",
    "e2e:server.debug.ingress": "pnpm run e2e:end.debug && mkdir -p  'e2e/modbus2mqtt.temp/modbus2mqtt' && echo 'httpport: 3004\nsupervisor_host: localhost:3006' >e2e/modbus2mqtt.temp/modbus2mqtt/modbus2mqtt.yaml && pnpm run build ",
    "e2e:server.debug": "pnpm run e2e:end.debug && mkdir -p  'e2e/modbus2mqtt.temp/modbus2mqtt' && echo 'httpport: 3005' >e2e/modbus2mqtt.temp/modbus2mqtt/modbus2mqtt.yaml && pnpm run build ",
    "e2e:end.debug": "rm -rf 'e2e/modbus2mqtt.temp'",
    "build": "pnpm run --silent build:backend && pnpm run --silent build:frontend",
    "build:backend": "cd backend && pnpm run build",
    "build:frontend": "cd frontend && pnpm run build",
    "prettier": "./scripts/prettier-changed.sh",
    "prettier:write": "prettier --write .",
    "install-hooks": "mkdir -p .githooks && ln -sf ../scripts/pre-commit.sh .githooks/pre-commit && chmod +x .githooks/pre-commit && git config core.hooksPath .githooks && echo 'Git hooks configured to use .githooks directory and linked to scripts/pre-commit.sh'",
    "test:spec-install": "bash scripts/test-spec-install.sh"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "async-mutex": "^0.5.0",
    "bcryptjs": "^3.0.3",
    "bootstrap": "^5.3.3",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "express": "^5.0.1",
    "express-fileupload": "^1.5.1",
    "http-server": "^14.1.1",
    "jsonwebtoken": "^9.0.2",
    "modbus-serial": "^8.0.17",
    "mqtt": "^5.9.1",
    "multer": "^2.0.2",
    "node-html-parser": "^7.0.1",
    "rxjs": "^7.8.2",
    "serve-static": "^2.2.0",
    "url-parse": "^1.5.10",
    "winston": "^3.18.3",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@babel/preset-typescript": "^7.27.1",
    "@opengovsg/credits-generator": "^1.0.7",
    "@playwright/test": "^1.58.2",
    "@types/adm-zip": "^0.5.5",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.5.5",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.2",
    "@types/url-parse": "^1.4.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.4",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-summarizing-reporter": "^1.1.4",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "typescript-map": "^0.1.0",
    "vitest": "^4.0.12"
  }
}
